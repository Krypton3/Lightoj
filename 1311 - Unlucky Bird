#include<bits/stdc++.h>
using namespace std;
int main()
{
    int test;
    double v1,v2,v3,a1,a2;
    scanf("%d",&test);
    for(int p=1;p<=test;p++){
        scanf("%lf%lf%lf%lf%lf",&v1,&v2,&v3,&a1,&a2);
        double res=v1*v1/2/a1+v2*v2/2/a2;
        double t1=v1/a1;
        double t2=v2/a2;
        double time=max(t1,t2);
        double res2=time*v3;
        printf("Case %d: %.6lf %.6lf\n",p,res,res2);
    }
    return 0;
}
